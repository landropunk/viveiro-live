# Variables de entorno de ejemplo
# Copia este archivo a .env.local para desarrollo local

# Supabase
# Obtén estas credenciales en: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://tu-proyecto.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=tu_anon_key_aqui

# MeteoGalicia API V5
# Obtén tu API key en: https://www.meteogalicia.gal/web/servizos/
# La aplicación utiliza la API V5 para mayor precisión (grid 1km)
METEOGALICIA_API_KEY=tu-api-key-de-meteogalicia-aqui

# OAuth Providers Habilitados
# Activa solo los proveedores que hayas configurado en Supabase
# Para configurar cada proveedor, sigue la guía en OAUTH_SETUP.md
NEXT_PUBLIC_OAUTH_GOOGLE_ENABLED=false
NEXT_PUBLIC_OAUTH_FACEBOOK_ENABLED=false
NEXT_PUBLIC_OAUTH_APPLE_ENABLED=false
NEXT_PUBLIC_OAUTH_MICROSOFT_ENABLED=false

# Node Environment
NODE_ENV=development
